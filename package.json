{
  "name": "frontend-template-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "style:all": "tsc && npm run style:lint && npm run style:prettier",
    "style:lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install",
    "release": "release-it"
  },
  "dependencies": {
    "@badrap/bar-of-progress": "^0.2.2",
    "@next/bundle-analyzer": "^13.2.4",
    "@socialgouv/matomo-next": "^1.6.1",
    "cookies-next": "^2.1.1",
    "next": "13.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "release-it": "^15.9.0",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.5",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1"
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
